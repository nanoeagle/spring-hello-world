<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="fooOne" class="com.example.helloworld.autowiring.FooImplOne"/>
  <bean id="fooTwo" primary="true"
    class="com.example.helloworld.autowiring.FooImplTwo"/>
  <bean id="fooThree" class="com.example.helloworld.autowiring.FooImplThree"/>

  <bean id="barOne" class="com.example.helloworld.autowiring.Bar"/>

  <bean id="targetByName" autowire="byName"
    class="com.example.helloworld.autowiring.Target" lazy-init="true"/>
    
  <bean id="targetByType" autowire="byType"
    class="com.example.helloworld.autowiring.Target" lazy-init="true"/>
    
  <bean id="targetConstructor" autowire="constructor"
    class="com.example.helloworld.autowiring.Target" lazy-init="true"/>
</beans>